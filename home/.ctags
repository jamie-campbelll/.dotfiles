--exclude=.git
--exclude=.vs
--exclude=packages
--exclude=vendor
--exclude=node_modules
--exclude=bin
--exclude=obj

--langmap=Lisp:+.clj

--langdef=cakebuild
--langmap=cakebuild:.cake
--regex-cakebuild=/^.*Task[ \t]*\(\"(.*)\"\)/\1/t,Task/

--langdef=markdown
--langmap=markdown:.mkd
--regex-markdown=/^#[ \t]+(.*)/\1/h,Heading_L1/
--regex-markdown=/^##[ \t]+(.*)/\1/i,Heading_L2/
--regex-markdown=/^###[ \t]+(.*)/\1/k,Heading_L3/

--langdef=XSD
--langmap=XSD:.xsd
--regex-XSD=/^\s*<.*element.+name=["']([a-zA-Z0-9_-]+)["']/\1/e,element/
--regex-XSD=/^\s*<.*complexType.+name=["']([a-zA-Z0-9_-]+)["']/\1/c,complexType/
--regex-XSD=/^\s*<.*simpleType.+name=["']([a-zA-Z0-9_-]+)["']/\1/s,simpleType/
